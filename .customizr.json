{
    "cache": true,
    "devFile": false,
    "dest": "public/assets/js/vendor/modernizr.js",
    "options": [
        "addTest",
        "html5printshiv",
        "testProp"
    ],
    "quiet": false,
    "uglify": true,
    "tests": [],
    "excludeTests": [],
    "crawl": true,
    "useBuffers": false,
    "files": {
        "src": [
            "./dist/*.{js,css,scss,html}",
            "*[^(g|G)runt(file)?].{js,css,scss}",
            "**[^node_modules]/**/*.{js,css,scss}",
            "!lib/**/*"
        ]
    },
    "customTests": [],
    "classPrefix": ""
}
